<%- include("partials/header") -%>
<div class="heading">
  <h1>Compose</h1>
</div>
<form action="/compose" method="post">
  <div class="container">
    <div class="form-set">
      <label for="postTitle">Title</label>
      <input type="text" name="postTitle" placeholder="Write Here" />
      <label for="postBody">Post</label>
      <textarea name="postBody" id="" cols="60" rows="10"></textarea>
    </div>

    <button class="btn" type="submit" name="button" value="compose">
      Publish
    </button>
  </div>
</form>

<%- include("partials/footer") -%>
